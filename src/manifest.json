{
  "manifest_version": 2,
  "name": "Putman PoC",
  "version": "1.0.0",
  "description": "Putman project",
  "author": "Drew Xiu",
  "icons": {
    "16": "assets/img/icon.jpg",
    "48": "assets/img/icon.jpg",
